{
  "name": "js-game",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "autoprefixer": "^10.4.20",
    "axios": "^1.7.4",
    "bcryptjs": "^2.4.3",
    "crypto": "^1.0.1",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.9.14",
    "postcss": "^8.4.41",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.1",
    "react-scripts": "5.0.1",
    "tailwindcss": "^3.4.10",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "concurrently \"npm run tailwind:watch\" \"react-scripts start\"",
